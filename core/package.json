{
  "name": "@repo/core",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "tsc && node ../build/core/src/index.js",
    "crypto": "tsc && node ../build/core/src/crypto.js",
    "agent": "tsc && node ../build/core/src/agent.js",
    "check-types": "tsc --noEmit"
  },
  "private": true,
  "author": "",
  "license": "ISC",
  "description": "",
  "exports": {
    "./agent": {
      "default": "./src/agent.ts",
      "types": "./src/build/agent.d.ts",
      "require": "./src/build/agent.js",
      "node": "./src/build/agent.js"
    },
    "./components": {
      "default": "./src/components.ts",
      "types": "./src/build/components.d.ts",
      "require": "./src/build/components.js",
      "node": "./src/build/components.js"
    },
    "./db": {
      "default": "./src/db.ts",
      "types": "./src/build/db.d.ts",
      "require": "./src/build/db.js",
      "node": "./src/build/db.js"
    },
    "./llms": {
      "default": "./src/llms.ts",
      "types": "./src/build/llms.d.ts",
      "require": "./src/build/llms.js",
      "node": "./src/build/llms.js"
    },
    "./crypto": {
      "default": "./src/crypto.ts",
      "types": "./src/build/crypto.d.ts",
      "require": "./src/build/crypto.js",
      "node": "./src/build/crypto.js"
    }
  },
  "devDependencies": {
    "@repo/tsconfig": "*",
    "@types/express": "^5.0.3",
    "@types/node": "^24.2.0",
    "@types/pg": "^8.15.4"
  },
  "dependencies": {
    "@langchain/core": "^0.3.70",
    "@langchain/google-genai": "^0.2.16",
    "@langchain/langgraph": "^0.4.4",
    "@langchain/ollama": "^0.2.3",
    "@langchain/redis": "^0.1.1",
    "@repo/db": "^1.0.0",
    "ioredis": "^5.7.0",
    "langchain": "^0.3.30",
    "mysql2": "^3.14.3",
    "ollama": "^0.5.16",
    "pg": "^8.16.3",
    "zod": "^4.0.15"
  }
}
