{
  "name": "@repo/db",
  "version": "1.0.0",
  "description": "",
  "license": "ISC",
  "author": "",
  "type": "module",
  "scripts": {
    "build": "npx prisma generate && tsc && cp -r ./migrations ./build/migrations ",
    "deploy": "npx prisma migrate deploy",
    "dev": "tsc --watch"
  },
  "exports": {
    ".": {
      "default": "./src/handler.ts",
      "node": "./build/src/handler.js",
      "types": "./build/src/handler.d.ts"
    },
    "./client": {
      "default": "./src/client.ts",
      "node": "./build/src/client.js",
      "types": "./build/src/client.d.ts"
    },
    "./adapter": {
      "default": "./src/adapter.ts",
      "node": "./build/src/adapter.js",
      "types": "./build/src/adapter.d.ts"
    },
    "./esm.handler": {
      "default": "./src/esm.handler.ts",
      "node": "./build/src/esm.handler.js",
      "types": "./build/src/esm.handler.d.ts"
    },
    "./esm": {
      "default": "./src/esm.ts",
      "node": "./build/src/esm.js",
      "types": "./build/src/esm.d.ts"
    }
  },
  "devDependencies": {
    "prisma": "^6.14.0",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@prisma/client": "^6.14.0",
    "@repo/tsconfig": "^0.0.0",
    "dotenv": "^17.2.1"
  }
}
